  <PropertyGroup>
    <Authors>EF Education First</Authors>
    <PackageProjectUrl>https://github.com/ef-labs/slf4net</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Copyright>Copyright Â© 2020 EF Learning Labs</Copyright>
    <AllowedOutputExtensionsInPackageBuildOutputFolder>$(AllowedOutputExtensionsInPackageBuildOutputFolder);.pdb</AllowedOutputExtensionsInPackageBuildOutputFolder>

    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>slf4net.snk</AssemblyOriginatorKeyFile>
